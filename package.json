{
  "name": "obsidian-scribe-plugin",
  "version": "2.1.1",
  "description": "An Obsidian plugin for recording voice notes, transcribing the audio, and summarizing the text - All in one",
  "main": "build/main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build:prod": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "version": "node version-bump.mjs && git add manifest.json versions.json",
    "update-all-deps": "npm outdated | awk 'NR>1 {print $1\"@\"$4}' | xargs npm install"
  },
  "keywords": [],
  "author": "Mike Alicea",
  "license": "MIT",
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@types/node": "^22.15.3",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.3",
    "builtin-modules": "^5.0.0",
    "dotenv": "^16.4.7",
    "esbuild": "^0.25.0",
    "obsidian": "latest",
    "tslib": "^2.8.1",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@ffmpeg/ffmpeg": "^0.12.15",
    "@ffmpeg/util": "^0.12.2",
    "@fix-webm-duration/fix": "^1.0.1",
    "@langchain/core": "^0.3.40",
    "@langchain/google-genai": "^0.2.18",
    "@langchain/openai": "^0.5.10",
    "assemblyai": "^4.9.0",
    "langchain": "^0.3.19",
    "mini-debounce": "^1.0.8",
    "openai": "^4.85.4",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "zod": "^3.24.2"
  }
}
